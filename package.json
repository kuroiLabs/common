{
  "name": "@kuroi/common",
  "version": "1.0.5",
  "description": "Common library for all kuroiLabs TypeScript projects",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "copyPkgJson": "shx cp package.json lib/package.json",
    "copyTypes": "shx cp -rf src/types lib/types",
    "build": "shx rm -rf lib && tsc && npm run copyPkgJson && npm run copyTypes"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuroiLabs/common.git"
  },
  "keywords": [
    "typescript",
    "utility"
  ],
